{
  "preset": "symfony",
  "rules": {
    "blank_line_between_import_groups": false,
    "ordered_imports": {
      "sort_algorithm": "length",
      "imports_order": ["const", "class", "function"]
    },
    "no_empty_comment": false,
    "not_operator_with_successor_space": true
  },
  "exclude": ["var"]
}
